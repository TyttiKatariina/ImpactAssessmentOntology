# GDPR High Risk Activities
dpia:SystematicProfiling a dpia:HighRiskActivity ;
    skos:prefLabel "Systematic and extensive profiling"@en ;
    dpia:requiresDPIA "true"^^xsd:boolean ;
    dpia:hasLegalBasis dpia:GDPR ;
    skos:definition "Systematic and extensive evaluation of personal aspects based on automated processing, including profiling"@en .

dpia:LargeScaleSpecialCategories a dpia:HighRiskActivity ;
    skos:prefLabel "Large scale processing of special categories of data"@en ;
    dpia:requiresDPIA "true"^^xsd:boolean ;
    dpia:hasLegalBasis dpia:GDPR ;
    skos:definition "Large scale processing of special categories of data or data relating to criminal convictions"@en .

# EDPB High Risk Activities
dpia:InnovativeTechnology a dpia:HighRiskActivity ;
    skos:prefLabel "Innovative technology or novel application"@en ;
    dpia:requiresDPIA "true"^^xsd:boolean ;
    dpia:hasLegalBasis dpia:EDPBGuidelines ;
    skos:definition "Use of innovative technologies or novel applications of existing technologies"@en .

dpia:SystematicMonitoring a dpia:HighRiskActivity ;
    skos:prefLabel "Systematic monitoring of public areas"@en ;
    dpia:requiresDPIA "true"^^xsd:boolean ;
    dpia:hasLegalBasis dpia:EDPBGuidelines ;
    skos:definition "Systematic monitoring of publicly accessible areas on a large scale"@en .

# Country-Specific High Risk Activities (Example for Austria)
dpia:AT_BiometricProcessing a dpia:HighRiskActivity ;
    skos:prefLabel "Biometric data processing"@en ;
    dpia:requiresDPIA "true"^^xsd:boolean ;
    dpia:hasJurisdiction dpia:Austria ;
    skos:definition "Processing of biometric data for uniquely identifying natural persons"@en .
